   <children>
      <TextField fx:id="uname" layoutX="274.0" layoutY="70.0" promptText="user name" />
      <TextField fx:id="pass" layoutX="265.0" layoutY="120.0" promptText="password" />
      <Button fx:id="register" layoutX="274.0" layoutY="187.0" mnemonicParsing="false" onAction="#onClickRegister" text="Register" />
   </children>
</AnchorPane>
